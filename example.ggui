!GGUIML
# Presume our window is rectangular
top-left top-left[1%, 1%, 1%, 1%] 70%x100% window 'data_window'
	TYPEARG
		header='Census'
		closable=off
		movable=off
	50%xDYNAMIC table
		TYPEARG rows-columns=DYNAMICx2
		MODULE 'entry' entry_header internal_name?
			label
				TYPEARG text=@entry_name
			label
				TYPEARG text="$census-chart:@TYPEARG.data.!{@entry_name}{@internal_name}"
		'header' label
			TYPEARG text='Continent'
		'header' label
			TYPEARG text='Populace'
		IMPORT $entry 'North America' 'AmericaNorth'
		IMPORT $entry 'South America' 'AmericaSouth'
		IMPORT $entry 'Europe'
		IMPORT $entry 'Asia'
		IMPORT $entry 'Africa'
		IMPORT $entry 'Antarctica'
		IMPORT $entry 'Oceania'
	label 'census_summary'
		TYPEARG text='Our census estimate for this year shows many of our users are overseas. There are more users present in Oceania that we are serving to than North America. It may be wise to invest in legal representatives abroad as our organization reaches more people.'
top-right top[1%, 1%, 1%, 1%] 30%x100% window 'graph_window'
	TYPEARG header='Display' resizable=off
	## A bar chart with several points to display the census.
	80%x80% 'bar-chart' graph 'census-chart'
		TYPEARG
			data=
				- 'AmericaNorth': 543
				- 'AmericaSouth': 3641
				- 'Europe': 928
				- 'Asia': 465
				- 'Africa': 5957
				- 'Antarctica': 0
				- 'Oceania': 1263